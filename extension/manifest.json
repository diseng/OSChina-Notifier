{
	"name": "OSChina Notifier",
	"description": "Displays your OSChina notifications unread count",
	"version": "0.0.2",
	"background": {
		"scripts": [
			"api.js",
			"main.js"
		]
	},
	"permissions": [
		"http://www.oschina.net/"
	],
	"browser_action": {
		"default_icon": "icon-19.png"
	},
	"icons": {
		"16": "icon-16.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"minimum_chrome_version": "18",
	"manifest_version": 2
}
